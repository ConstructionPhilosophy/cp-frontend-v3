# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id_here
VITE_FIREBASE_APP_ID=your_firebase_app_id_here

# API Configuration
# Main API base URL - defaults to current domain origin if not set
VITE_API_BASE_URL=https://your-api-domain.com
# Geo API base URL - defaults to current domain origin if not set  
VITE_GEO_API_BASE_URL=https://your-geo-api-domain.com

# Development note: 
# If these environment variables are not set, the app will default to using 
# window.location.origin which makes it work on any deployed domain